{
  "name": "tictactwo",
  "version": "1.0.0",
  "description": "Tic Tac Toe",
  "main": "game/ttt.js",
  "scripts": {
    "start": "npm run build && http-server dist",
    "test": "standard",
    "clean": "rm -rf dist",
    "build": "npm run clean && mkdir dist && npm run build-html && npm run build-css && npm run build-js",
    "build-html": "cp game/index.html dist/index.html",
    "build-css": "cp game/styles.css dist/styles.css",
    "build-js": "browserify game/ttt.js --outfile dist/ttt.dist.js --transform babelify"
  },
  "keywords": [
    "tictactwo"
  ],
  "author": "@wytanj",
  "repository": {
    "type": "git",
    "url": "https://github.com/wytanj/tictactwo.git"
  },
  "license": "ISC",
  "dependencies": {
    "babel-preset-es2015": "^6.1.2",
    "babel-preset-stage-0": "^6.1.2",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "http-server": "^0.8.5",
    "standard": "^5.3.1"
  }
}
